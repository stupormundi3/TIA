        etat(NewPositionsNewNbPonts,PositionsPonts),
        descendre):-
            


transition(etat(PositionsLutins,NbPonts,PositionsPonts),
    etat(NewPositionsNewNbPonts,PositionsPonts),
                droite):-



transition(etat(PositionsBleu,PositionsVert,PositionsRouge,PositionsJaune,NbPonts,PositionsPonts),
                    etat(PositionsBleu,PositionsVert,NewPositionsRouge,PositionsJaune,NewNbPonts,PositionsPonts),
                        gauche):-
                                                                 



